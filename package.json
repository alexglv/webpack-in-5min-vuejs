{
  "name": "webpack-in-5min-vuejs",
  "version": "1.2.0",
  "description": "A minimalistic setup suitable for basic development using Webpack (with Vue.js + Vuex + vue-router).",
  "main": "build/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run serve",
    "lint": "node ./node_modules/eslint/bin/eslint.js --rulesdir . ./src ./webpack.config.babel.js -f table --ext .js || true",
    "serve": "webpack-dev-server --env.dev --progress --profile --colors --bail",
    "build": "webpack --env.prod --progress --profile --colors",
    "dev": "npm run serve",
    "prod": "npm run build"
  },
  "keywords": [
    "webpack",
    "vuejs",
    "vuex",
    "vue-router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/minagawah/webpack-in-5min-vuejs.git"
  },
  "author": "minagawah",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.0",
    "lodash.merge": "^4.6.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "uglify-save-license": "^0.4.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-dev-server": "^2.3.0",
    "yargs": "^6.6.0"
  },
  "dependencies": {
    "seedrandom": "^2.4.2",
    "three": "^0.84.0",
    "vue": "^2.1.10",
    "vue-resource": "^1.2.0",
    "vue-router": "^2.2.0",
    "vuex": "^2.1.2"
  }
